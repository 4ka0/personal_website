#: env/lib/python3.7/site-packages/django/contrib/postgres/apps.py:42
msgid "PostgreSQL extensions"
msgstr "PostgreSQL拡張"

#: env/lib/python3.7/site-packages/django/contrib/postgres/fields/array.py:21
#: env/lib/python3.7/site-packages/django/contrib/postgres/forms/array.py:16
#: env/lib/python3.7/site-packages/django/contrib/postgres/forms/array.py:170
#, python-format
msgid "Item %(nth)s in the array did not validate:"
msgstr "配列内のアイテム %(nth)s は検証できませんでした:"

#: env/lib/python3.7/site-packages/django/contrib/postgres/fields/array.py:22
msgid "Nested arrays must have the same length."
msgstr "ネストした配列は同じ長さにしなければなりません。"

#: env/lib/python3.7/site-packages/django/contrib/postgres/fields/hstore.py:16
msgid "Map of strings to strings/nulls"
msgstr "文字列と文字列/NULLのマップ"

#: env/lib/python3.7/site-packages/django/contrib/postgres/fields/hstore.py:18
#, fuzzy, python-format
#| msgid "The value of \"%(key)s\" is not a string or null."
msgid "The value of “%(key)s” is not a string or null."
msgstr "\"%(key)s\" の値は文字列または NULL ではありません。"

#: env/lib/python3.7/site-packages/django/contrib/postgres/fields/jsonb.py:32
msgid "A JSON object"
msgstr "JSONオブジェクト"

#: env/lib/python3.7/site-packages/django/contrib/postgres/fields/jsonb.py:34
msgid "Value must be valid JSON."
msgstr "JSONとして正しい値にしてください。"

#: env/lib/python3.7/site-packages/django/contrib/postgres/forms/hstore.py:16
msgid "Could not load JSON data."
msgstr "JSONデータを読み込めませんでした。"

#: env/lib/python3.7/site-packages/django/contrib/postgres/forms/hstore.py:17
msgid "Input must be a JSON dictionary."
msgstr "JSON辞書を入力しなければなりません。"

#: env/lib/python3.7/site-packages/django/contrib/postgres/forms/jsonb.py:19
#, fuzzy, python-format
#| msgid "'%(value)s' value must be valid JSON."
msgid "“%(value)s” value must be valid JSON."
msgstr "'%(value)s' は正しいJSONにしなければなりません。"

#: env/lib/python3.7/site-packages/django/contrib/postgres/forms/ranges.py:37
msgid "Enter two valid values."
msgstr "2つの値を正しく入力してください。"

#: env/lib/python3.7/site-packages/django/contrib/postgres/forms/ranges.py:38
msgid "The start of the range must not exceed the end of the range."
msgstr "範囲の開始は、範囲の終わりを超えてはなりません。"

#: env/lib/python3.7/site-packages/django/contrib/postgres/forms/ranges.py:86
msgid "Enter two whole numbers."
msgstr "2つの整数を入力してください。"

#: env/lib/python3.7/site-packages/django/contrib/postgres/forms/ranges.py:92
msgid "Enter two numbers."
msgstr "2つの数値を入力してください。"

#: env/lib/python3.7/site-packages/django/contrib/postgres/forms/ranges.py:109
msgid "Enter two valid date/times."
msgstr "2つの日付/時間を入力してください。"

#: env/lib/python3.7/site-packages/django/contrib/postgres/forms/ranges.py:115
msgid "Enter two valid dates."
msgstr "2つの日付を入力してください。"

#: env/lib/python3.7/site-packages/django/contrib/postgres/validators.py:12
#, python-format
msgid ""
"List contains %(show_value)d item, it should contain no more than "
"%(limit_value)d."
msgid_plural ""
"List contains %(show_value)d items, it should contain no more than "
"%(limit_value)d."
msgstr[0] ""
"リストには%(show_value)d個のアイテムが含まれますが、%(limit_value)d個までしか"
"含められません。"

#: env/lib/python3.7/site-packages/django/contrib/postgres/validators.py:19
#, python-format
msgid ""
"List contains %(show_value)d item, it should contain no fewer than "
"%(limit_value)d."
msgid_plural ""
"List contains %(show_value)d items, it should contain no fewer than "
"%(limit_value)d."
msgstr[0] ""
"リストには%(show_value)d個のアイテムが含まれますが、%(limit_value)d個までしか"
"含められません。"

#: env/lib/python3.7/site-packages/django/contrib/postgres/validators.py:29
#, python-format
msgid "Some keys were missing: %(keys)s"
msgstr "いくつかのキーが欠落しています: %(keys)s"

#: env/lib/python3.7/site-packages/django/contrib/postgres/validators.py:30
#, python-format
msgid "Some unknown keys were provided: %(keys)s"
msgstr "いくつかの不明なキーがあります: %(keys)s"

#: env/lib/python3.7/site-packages/django/contrib/postgres/validators.py:70
#, python-format
msgid ""
"Ensure that this range is completely less than or equal to %(limit_value)s."
msgstr "この範囲が完全に%(limit_value)s以下であることを確認してください。"

#: env/lib/python3.7/site-packages/django/contrib/postgres/validators.py:76
#, python-format
msgid ""
"Ensure that this range is completely greater than or equal to "
"%(limit_value)s."
msgstr "この範囲が完全に%(limit_value)s以上であることを確認してください。"
